package programs;

import java.util.Scanner;

public class Amst {

	public static void main(String[] args) {

		
		Scanner sc= new Scanner(System.in);
		System.out.println("Please enter no to be whether it is Armstrong or not");
		int number=sc.nextInt();
		
		int sum=0;
		
		int temp=number;
		while(temp!=0){
			
			int rem=temp%10;
			int digitcube=rem*rem*rem;
			System.out.println("("+digitcube+")");
			sum=sum+rem*rem*rem;
			temp=temp/10;
		}
		
		if(sum==number) {
			
			System.out.println("Number is an Armstrong no");
		}
		else {
			System.out.println("Number is not an Armstrong no");
		}
		
		
		
		
		
		
	}

}
