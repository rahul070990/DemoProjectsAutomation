package programs;

public class SeriesArmstrong {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		int sum=0;
		int temp;
		for(int i=1;i<=1000;i++)
			temp=i;
			while(temp!=0){
			
			int rem=temp%10;
			sum=sum+rem*rem*rem;
			temp=temp/10;
			
		}
		
		sum=0;
		
	}

}
